{
  "buildCommand": "pnpm build",
  "devCommand": "pnpm dev",
  "installCommand": "pnpm install",
  "framework": "nextjs",
  "regions": [
    "sin1"
  ],
  "env": {
    "NEXT_PUBLIC_APP_URL": "https://sedia-pos.vercel.app",
    "BETTER_AUTH_URL": "https://sedia-pos.vercel.app"
  },
  "build": {
    "env": {
      "NEXT_TELEMETRY_DISABLED": "1"
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    }
  ]
}