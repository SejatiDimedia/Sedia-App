{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@shared-db": [
        "../../shared-db/src/index"
      ],
      "@shared-db/*": [
        "../../shared-db/src/*"
      ]
    }
  }
}